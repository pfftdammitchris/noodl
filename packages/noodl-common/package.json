{
	"author": "Christopher Tran <pfftdammitchris@gmail.com>",
	"bugs": {
		"url": "https://github.com/pfftdammitchris/noodl-cli/issues"
	},
	"dependencies": {
		"mime-types": "^2.1.31",
		"minimatch": "^3.0.4"
	},
	"description": "Common NOODL file system utilities",
	"devDependencies": {
		"@types/mime-types": "^2.1.0"
	},
	"exports": {
		"default": "./dist/noodl-common.cjs",
		"import": "./dist/noodl-common.js",
		"require": "./dist/noodl-common.cjs"
	},
	"files": [
		"dist"
	],
	"gitHead": "c6d5712c14cedf5faf330797187ffff96b9ce6ee",
	"homepage": "https://github.com/pfftdammitchris/noodl-cli#readme",
	"license": "ISC",
	"name": "noodl-common",
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/pfftdammitchris/noodl-cli.git"
	},
	"scripts": {
		"build": "concurrently \"rollup -c \" \"npm run types\"",
		"start": "cross-env NODE_ENV=development concurrently \"rollup -c -w\" \"npm run types\"",
		"test": "cross-env NODE_ENV=test ts-mocha -p tsconfig.test.json --loader=ts-node/esm --extensions ts --watch --watch-files src \"src/**/*.test.ts\"",
		"types": "tsc --emitDeclarationOnly"
	},
	"type": "module",
	"types": "./dist/noodl-common.d.ts",
	"typesVersions": {
		"*": {
			"index.d.ts": [
				"./dist/noodl-common.d.ts"
			]
		}
	},
	"version": "0.0.58"
}
