  "dependencies": {
    "aws-lambda": "^1.0.6",
    "pg": "^8.7.1"
  },
  "description": "Metadata for noodl applications",
  "devDependencies": {
    "@netlify/functions": "^0.7.2",
    "@types/aws-lambda": "^8.10.83",
    "@types/pg": "^8.6.1"
  },
  "directories": {
    "dist": "dist",
    "test": "__tests__"
  },
  "exports": {
