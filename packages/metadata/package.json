  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/pfftdammitchris/noodl#readme",
  "keywords": [
    "noodl",
    "ecos",
    "metadata",
    "telemedicine"
  ],
  "license": "ISC",
  "main": "./dist/metadata.js",
  "module": "./dist/metadata.js",
  "name": "metadata",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pfftdammitchris/noodl-metadata.git"
  },
  "scripts": {
    "build": "npm run tsc --project tsconfig.json",
    "prebuild": "rm -rf dist",
    "start": "npm run tsc -- -w",
    "start:funcs": "netlify dev",
    "test": "cross-env NODE_ENV=test ts-mocha -p tsconfig.test.json --extensions ts --watch --watch-files src \"src/**/*.test.ts\"",
    "tsc": "tsc"
  },
  "type": "module",
  "typesVersions": {
    "*": {
      "metadata.d.ts": [
        "./dist/metadata.d.ts"
