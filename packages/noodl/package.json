{
	"author": "Christopher Tran <pfftdammitchris@gmail.com>",
	"bugs": {
		"url": "https://github.com/pfftdammitchris/noodl-cli/issues"
	},
	"description": "NOODL is a language stemmed from the YAML language. This library gives you the tools to build JavaScript applications using Neural Object Oriented Descriptive Language",
	"exports": {
		"default": "./dist/index.js",
		"import": "./dist/index.js",
		"require": "./dist/index.js"
	},
	"files": [
		"dist"
	],
	"gitHead": "c6d5712c14cedf5faf330797187ffff96b9ce6ee",
	"homepage": "https://github.com/pfftdammitchris/noodl-cli#readme",
	"keywords": [
		"noodl",
		"yaml",
		"ecos",
		"edge",
		"vertex",
		"aitmed",
		"telemedicine",
		"health"
	],
	"license": "ISC",
	"main": "./dist/noodl.js",
	"module": "./dist/noodl.js",
	"name": "noodl",
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/pfftdammitchris/noodl-cli.git"
	},
	"scripts": {
		"prebuild": "rm -rf dist",
		"build": "cross-env NODE_ENV=production concurrently \"npm run build:js\" \"npm run build:types\"",
		"build:esm": "esbuild src/index.ts --outdir=dist --sourcemap --platform=node --target=es2020 --format=esm",
		"build:cjs": "esbuild src/index.ts --outdir=dist/cjs --sourcemap --platform=node --target=es2020 --format=cjs",
		"build:js": "rollup -c",
		"build:types": "tsc --emitDeclarationOnly --skipLibCheck",
		"start": "cross-env NODE_ENV=development concurrently \"npm run build:js -- --watch\" \"npm run build:types -- -w\"",
		"test": "cross-env NODE_OPTIONS='--experimental-vm-modules --no-warnings' jest --watch"
	},
	"type": "module",
	"typesVersions": {
		"*": {
			"index.d.ts": [
				"./dist/index.d.ts"
			]
		}
	},
	"typings": "./dist/noodl.d.ts",
	"version": "0.0.7"
}
