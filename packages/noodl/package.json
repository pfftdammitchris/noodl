{
    "author": "Christopher Tran <pfftdammitchris@gmail.com>",
    "bugs": {
        "url": "https://github.com/pfftdammitchris/noodl-cli/issues"
    },
    "description": "NOODL is a language stemmed from the YAML language. This library gives you the tools to build JavaScript applications using Neural Object Oriented Descriptive Language",
    "exports": {
        "default": "./dist/noodl.cjs.js",
        "import": "./dist/noodl.es.js",
        "require": "./dist/noodl.cjs.js"
    },
    "files": [
        "dist"
    ],
    "gitHead": "c6d5712c14cedf5faf330797187ffff96b9ce6ee",
    "homepage": "https://github.com/pfftdammitchris/noodl-cli#readme",
    "keywords": [
        "noodl",
        "yaml",
        "ecos",
        "edge",
        "vertex",
        "aitmed",
        "telemedicine",
        "health"
    ],
    "license": "ISC",
    "name": "noodl",
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/pfftdammitchris/noodl-cli.git"
    },
    "scripts": {
        "build": "cross-env NODE_ENV=production concurrently \"rollup -c\" \"npm run types\"",
        "build:esbuild": "esbuild src/index.ts --banner:js='import { createRequire as topLevelCreateRequire } from \"module\";\n const require = topLevelCreateRequire(import.meta.url);' --bundle --sourcemap --platform=node --target=es2020",
        "prebuild": "rm -rf dist",
        "start": "cross-env NODE_ENV=development concurrently \"rollup -c -w\" \"npm run types\"",
        "test": "cross-env TS_NODE_PROJECT=\"tsconfig.test.json\" NODE_ENV=test ts-mocha --extensions ts \"src/**/*.ts\"",
        "test:file": "cross-env TS_NODE_PROJECT=\"tsconfig.test.json\" NODE_ENV=test ts-mocha --extensions ts --watch --watch-files src",
        "types": "tsc --declaration --emitDeclarationOnly"
    },
    "type": "module",
    "types": "./dist/noodl.d.ts",
    "typesVersions": {
        "*": {
            "noodl.d.ts": [
                "./dist/noodl.d.ts"
            ]
        }
    },
    "version": "0.0.8"
}
