{
	"bin": {
		"noodl": "./bin/cli.js"
	},
	"dependencies": {
		"chokidar": "^3.5.1",
		"configstore": "^6.0.0",
		"dotenv": "^10.0.0",
		"download": "^8.0.0",
		"express": "^4.17.1",
		"fast-xml-parser": "^3.19.0",
		"immer": "^9.0.6",
		"ink": "^3.0.9",
		"ink-big-text": "^1.2.0",
		"ink-gradient": "^2.0.0",
		"ink-multi-select": "^2.0.0",
		"ink-progress-bar": "^3.0.0",
		"ink-select-input": "^4.2.0",
		"ink-spinner": "^4.0.1",
		"ink-text-input": "^4.0.1",
		"invariant": "^2.2.4",
		"mime": "^2.5.2",
		"react": "^16.14.0",
		"ws": "^8.2.1"
	},
	"devDependencies": {
		"@types/configstore": "^5.0.1",
		"@types/download": "^8.0.1",
		"@types/express": "^4.17.13",
		"@types/fs-extra": "^9.0.12",
		"@types/ink-big-text": "^1.2.1",
		"@types/ink-gradient": "^2.0.1",
		"@types/ink-select-input": "^3.0.1",
		"@types/invariant": "^2.2.35",
		"@types/react": "^17.0.20",
		"@types/ws": "^7.4.7",
		"ink-testing-library": "^2.1.0",
		"node-cron": "^3.0.0"
	},
	"engines": {
		"node": ">=12"
	},
	"exports": {
		"import": "./cli.esm",
		"require": "./cli.cjs"
	},
	"files": [
		"./bin/",
		"./dist/",
		"./src/",
		"tsconfig.json",
		"tsconfig.test.json",
		"readme.md",
		".editorconfig",
		".gitconfig",
		".npmignore",
		"noodl.yml"
	],
	"license": "MIT",
	"name": "noodl-cli",
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/pfftdammitchris/noodl-cli"
	},
	"scripts": {
		"build": "cross-env NODE_ENV=production npm run build:esm && npm run build:cjs",
		"build:cjs": "tsc --module commonjs --target es2018 --outDir ./dist/cjs --declaration --declarationDir ./dist/cjs",
		"build:esm": "tsc --module es2020 --target es2020",
		"prebuild": "rm -rf dist",
		"start": "cross-env NODE_ENV=development npm run build:esm -- --watch",
		"start:cjs": "cross-env NODE_ENV=development npm run build:cjs -- --watch",
		"test:file": "cross-env TS_NODE_PROJECT=\"tsconfig.test.json\" NODE_ENV=test ts-mocha --extensions ts --watch --watch-files \"src/**/*.ts\""
	},
	"type": "module",
	"typesVersions": {
		"*": {
			"cli.d.ts": [
				"./dist/cli.d.ts"
			]
		}
	},
	"typings": "./dist/cli.d.ts",
	"version": "0.0.111"
}
