{
	"author": "Christopher Tran <pfftdammitchris@gmail.com>",
	"bugs": {
		"url": "https://github.com/pfftdammitchris/noodl/issues"
	},
	"dependencies": {},
	"description": "Noodl aggregator",
	"devDependencies": {
		"meow": "^9.0.0"
	},
	"engines": {
		"node": ">=14"
	},
	"files": [
		"dist"
	],
	"gitHead": "c6d5712c14cedf5faf330797187ffff96b9ce6ee",
	"homepage": "https://github.com/pfftdammitchris/noodl#readme",
	"keywords": [
		"noodl",
		"aggregator",
		"aggregate",
		"ecos",
		"aitmed",
		"telemedicine"
	],
	"license": "ISC",
	"main": "./dist/cjs/index.js",
	"module": "./dist/index.js",
	"name": "noodl-aggregator",
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/pfftdammitchris/noodl.git"
	},
	"scripts": {
		"build": "cross-env NODE_ENV=production npm run build:esm && npm run build:cjs",
		"build:cjs": "tsc --module commonjs --target es2015 --outDir ./dist/cjs --declaration --declarationDir ./dist/cjs",
		"build:esm": "tsc --module es2020 --target es2020",
		"start": "cross-env NODE_ENV=development npm run build:esm -- --watch",
		"start:cjs": "cross-env NODE_ENV=development npm run build:cjs -- --watch",
		"test": "cross-env NODE_ENV=test ts-mocha -p tsconfig.test.json --extensions ts --watch --watch-files src \"src/**/*.test.ts\"",
		"types": "tsc --emitDeclarationOnly"
	},
	"types": "./dist/index.d.ts",
	"typesVersions": {
		"*": {
			"cli.d.ts": [
				"./dist/cli.d.ts"
			]
		}
	},
	"version": "0.0.26",
	"workspaces": [
		"packages/*"
	]
}
