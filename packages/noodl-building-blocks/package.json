{
	"name": "noodl-building-blocks",
	"version": "0.0.29",
	"description": "NOODL loose building blocks",
	"author": "Christopher Tran <pfftdammitchris@gmail.com>",
	"homepage": "",
	"license": "ISC",
	"main": "dist/noodl-building-blocks.index.js",
	"types": "dist/index.d.ts",
	"engines": {
		"node": ">=12",
		"npm": ">=6"
	},
	"scripts": {
		"prebuild": "rm -rf dist",
		"build": "tsc",
		"prestart": "rm -rf dist",
		"start": "tsc --watch",
		"test": "cross-env NODE_ENV=test ts-mocha -p tsconfig.test.json -r ts-node/register --extensions ts --watch --watch-files src src/**/*.test.ts",
		"test:file": "cross-env NODE_ENV=test ts-mocha -p tsconfig.test.json -r ts-node/register --extensions ts --watch --watch-files src"
	},
	"files": [
		"dist"
	],
	"prettier": {
		"arrowParens": "always",
		"endOfLine": "lf",
		"printWidth": 80,
		"semi": false,
		"singleQuote": true,
		"tabWidth": 2,
		"trailingComma": "all"
	},
	"repository": {
		"type": "git",
		"url": "http://gitlab.aitmed.com/pfftdammitchris/aitmed-noodl-web.git"
	},
	"devDependencies": {
		"noodl-types": "file:../noodl-types"
	}
}
