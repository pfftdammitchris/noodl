{
	"author": "Christopher Tran <pfftdammitchris@gmail.com>",
	"bugs": {
		"url": "https://github.com/pfftdammitchris/noodl/issues"
	},
	"description": "Utility/helpers for working with NOODL values",
	"engines": {
		"node": ">=14",
		"npm": ">=7"
	},
	"exports": {
		"default": "./dist/noodl-utils.esm.js",
		"import": "./dist/noodl-utils.esm.js",
		"require": "./dist/noodl-utils.cjs.js"
	},
	"files": [
		"dist"
	],
	"gitHead": "c6d5712c14cedf5faf330797187ffff96b9ce6ee",
	"homepage": "https://github.com/pfftdammitchris/noodl#readme",
	"keywords": [
		"noodl"
	],
	"license": "ISC",
	"name": "noodl-utils",
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/pfftdammitchris/noodl.git"
	},
	"scripts": {
		"build": "cross-env NODE_ENV=production concurrently \"rollup -c\" \"npm run types\"",
		"prebuild": "rm -rf dist",
		"start": "cross-env NODE_ENV=development concurrently \"rollup -c -w\" \"npm run types\"",
		"test": "cross-env TS_NODE_PROJECT=\"tsconfig.test.json\" NODE_ENV=test ts-mocha -r jsdom-global/register --file src/__tests__/setup.ts -r source-map-support/register --extensions ts --watch --watch-files src src/**/*.test.ts",
		"test:file": "NODE_ENV=test ts-mocha -r jsdom-global/register --extensions ts --watch --watch-files src",
		"types": "tsc --emitDeclarationOnly --outFile dist/noodl-utils.d.ts"
	},
	"type": "module",
	"typesVersions": {
		"*": {
			"noodl-utils.d.ts": [
				"./dist/noodl-utils.d.ts"
			]
		}
	},
	"typings": "./dist/noodl-utils.d.ts",
	"version": "0.0.85"
}
