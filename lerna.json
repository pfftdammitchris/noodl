{
	"packages": ["packages/*"],
	"npmClient": "npm",
	"version": "independent",
	"command": {
		"run": {
			"ignore": ["generator", "noodl", "noodl-cli-docs", "protorepo"]
		},
		"bootstrap": {
			"npmClientArgs": []
		},
		"publish": {
			"ignore": [
				"generator",
				"noodl",
				"noodl-cli-docs",
				"protorepo",
				"*.md",
				"dev",
				"docs"
			]
		},
		"publishChanges": {}
	}
}
