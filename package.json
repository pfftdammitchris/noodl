  "scripts": {
    "bootstrap": "lerna bootstrap --hoist",
    "build": "lerna exec --scope noodl-cli \"npm run build\"",
    "clean": "rm -rf dist node_modules package-lock.json && lerna clean",
    "deploy:schema": "node scripts/uploadSchema.mjs",
    "postinstall": "node scripts/postinstall.mjs",
    "pub": "node scripts/publish.js -p",
    "ssh2": "ssh -i '../Downloads/ecos-noodl.pem' ubuntu@ec2-54-219-91-202.us-west-1.compute.amazonaws.com",
    "start": "lerna exec --scope noodl-cli npm run start",
    "start:schema": "npx nodemon scripts/uploadSchema.mjs --watch configuration/noodl.schema.json",
    "test:visitor": "cross-env TS_NODE_PROJECT=\"tsconfig.test.json\" NODE_ENV=test ts-mocha --extensions ts --watch --watch-files dev dev/referenceVisitor.test.ts"
  },
