		"agg": "nodemon scripts/metadata/Aggregator.mjs",
		"bootstrap": "lerna bootstrap --hoist",
		"build": "lerna exec --scope noodl-cli \"npm run build\"",
		"clean": "rm -rf dist node_modules package-lock.json && lerna clean",
		"postinstall": "node scripts/postinstall.js",
		"pub": "node scripts/publish.js -p",
		"dev": "npx ts-node-dev --respawn --transpile-only",
		"ssh2": "ssh -i '../Downloads/ecos-noodl.pem' ubuntu@ec2-54-219-91-202.us-west-1.compute.amazonaws.com",
		"start": "lerna exec --scope noodl-cli npm run start",
		"test": "web-test-runner \"scripts/metadata/**/*.test.js\" --node-resolve",
		"test:watch": "web-test-runner \"./scripts/metadata/**/*.test.js\" --node-resolve --watch"
	},
